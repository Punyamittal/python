f=open("f.txt","w")
n=int(input("enter number of lines"))
for i in range (0,n):
    l=input()
    f.write(l)
    f.write("\n")
f.close()
f=open("f.txt","r")
r=f.read()
print(r)
f=open("f.txt","a")
n=int(input("enter number of lines"))
for i in range (0,n):
    l=input()
    f.write(l)
    f.write("\n")
f=open("f.txt","r")
r=f.read()
print(r)
f=open("f.txt","a")
import os
os.remove("f.txt")
